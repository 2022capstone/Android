<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white"
            tools:context=".ui.CarRegisterFragment">

            <ImageView
                android:id="@+id/iv_register_car"
                android:layout_width="wrap_content"
                android:layout_height="180dp"
                android:layout_marginTop="16dp"
                android:src="@drawable/car_front"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/btn_car_register" />

            <Button
                android:id="@+id/btn_image_register"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:backgroundTint="@color/sub"
                android:fontFamily="@font/nanum"
                android:text="사진 등록하기"
                android:textColor="@color/white"
                app:layout_constraintEnd_toEndOf="@+id/iv_register_car"
                app:layout_constraintStart_toStartOf="@+id/iv_register_car"
                app:layout_constraintTop_toBottomOf="@+id/iv_register_car" />

            <TextView
                android:id="@+id/textView46"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="32dp"
                android:layout_marginTop="32dp"
                android:fontFamily="@font/nanum"
                android:text="차량번호"
                android:textColor="#000000"
                android:textSize="18sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/btn_image_register" />

            <EditText
                android:id="@+id/et_register_num"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="10"
                android:fontFamily="@font/nanum"
                android:hint="차량번호"
                android:inputType="textPersonName"
                android:minHeight="48dp"
                android:textSize="18sp"
                app:layout_constraintBottom_toBottomOf="@+id/textView46"
                app:layout_constraintStart_toStartOf="@+id/guideline2"
                app:layout_constraintTop_toTopOf="@+id/textView46"
                app:layout_constraintVertical_bias="0.611" />

            <TextView
                android:id="@+id/textView45"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="32dp"
                android:fontFamily="@font/nanum"
                android:text="차량모델"
                android:textColor="#000000"
                android:textSize="18sp"
                app:layout_constraintStart_toStartOf="@+id/textView46"
                app:layout_constraintTop_toBottomOf="@+id/et_register_num" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_begin="115dp" />

            <EditText
                android:id="@+id/et_register_model"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="10"
                android:fontFamily="@font/nanum"
                android:hint="차량모델"
                android:inputType="textPersonName"
                android:minHeight="48dp"
                android:textSize="18sp"
                app:layout_constraintBottom_toBottomOf="@+id/textView45"
                app:layout_constraintStart_toStartOf="@+id/guideline2"
                app:layout_constraintTop_toTopOf="@+id/textView45"
                app:layout_constraintVertical_bias="0.555" />

            <TextView
                android:id="@+id/textView47"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="32dp"
                android:fontFamily="@font/nanum"
                android:text="인승"
                android:textColor="#000000"
                android:textSize="18sp"
                app:layout_constraintStart_toStartOf="@+id/textView45"
                app:layout_constraintTop_toBottomOf="@+id/et_register_model" />

            <EditText
                android:id="@+id/et_register_seater"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="10"
                android:fontFamily="@font/nanum"
                android:hint="인승"
                android:inputType="numberSigned"
                android:minHeight="48dp"
                android:textSize="18sp"
                app:layout_constraintBottom_toBottomOf="@+id/textView47"
                app:layout_constraintStart_toStartOf="@+id/guideline2"
                app:layout_constraintTop_toTopOf="@+id/textView47" />

            <EditText
                android:id="@+id/et_register_starttime"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:ems="10"
                android:fontFamily="@font/nanum"
                android:hint="0000/00/00 00:00"
                android:inputType="textPersonName"
                android:minHeight="48dp"
                android:textSize="18sp"
                app:layout_constraintStart_toStartOf="@+id/textView48"
                app:layout_constraintTop_toBottomOf="@+id/textView48" />

            <TextView
                android:id="@+id/textView48"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="24dp"
                android:fontFamily="@font/nanum"
                android:text="사용가능기간"
                android:textColor="#000000"
                android:textSize="18sp"
                app:layout_constraintStart_toStartOf="@+id/textView47"
                app:layout_constraintTop_toBottomOf="@+id/et_register_location" />

            <Button
                android:id="@+id/btn_car_register"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:backgroundTint="@color/sub"
                android:fontFamily="@font/nanum"
                android:text="차량 등록하기"
                android:textColor="@color/white"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView63"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="32dp"
                android:fontFamily="@font/nanum"
                android:text="위치"
                android:textColor="#000000"
                android:textSize="18sp"
                app:layout_constraintStart_toStartOf="@+id/textView47"
                app:layout_constraintTop_toBottomOf="@+id/et_register_seater" />

            <EditText
                android:id="@+id/et_register_location"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="10"
                android:fontFamily="@font/nanum"
                android:hint="ㅇㅇ시 ㅇㅇ구 ㅇㅇ동"
                android:inputType="textPersonName"
                android:minHeight="48dp"
                android:textSize="18sp"
                app:layout_constraintBottom_toBottomOf="@+id/textView63"
                app:layout_constraintStart_toStartOf="@+id/guideline2"
                app:layout_constraintTop_toTopOf="@+id/textView63" />

            <EditText
                android:id="@+id/et_register_endtime"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="32dp"
                android:ems="10"
                android:fontFamily="@font/nanum"
                android:hint="0000/00/00 00:00"
                android:inputType="textPersonName"
                android:textSize="18sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="@+id/et_register_starttime"
                app:layout_constraintTop_toBottomOf="@+id/et_register_starttime" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
</layout>