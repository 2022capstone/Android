    <fragment
        android:id="@+id/myCarFragment"
        android:name="com.android.r.MyCarFragment"
        android:label="fragment_my_car"
        tools:layout="@layout/fragment_my_car" />
    <fragment
        android:id="@+id/usageDetailFragment"
        android:name="com.android.r.UsageDetailFragment"
        android:label="fragment_usage_detail"
        tools:layout="@layout/fragment_usage_detail" />
    <fragment
        android:id="@+id/chatFragment"
        android:name="com.android.r.ChatFragment"
        android:label="fragment_chat"
        tools:layout="@layout/fragment_chat" /><action android:id="@+id/action_global_myReservFragment" app:destination="@id/myReservFragment"/>
</navigation>